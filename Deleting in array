#include<iostream>
using namespace std;
void Delete (int arr[],int Size,int position);
int main ()
{
    int Size;
    cout<<"enter your Size \n";
    cin>>Size;
    int arr[Size];
    for(int i=0;i<Size;i++)
    {
        cin>>arr[i];
    }
    cout<<"enter the position do you want to delete the element from it \n";
    int position;
    cin>>position;
    Delete (arr,Size,position);
    Size=Size-1;

    for(int i=0;i<Size;i++)
    {
        cout<<arr[i];
    }
    return 0;
}
void Delete(int arr[],int Size,int position)
{
    for(int i=position;i<Size;i++)
    {
        arr[i]=arr[i+1];
    }
}
